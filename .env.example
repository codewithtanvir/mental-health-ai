# Environment Configuration for Mental Health AI

# Google Gemini API Key
# Get your API key from: https://makersuite.google.com/
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
# Get these from your Supabase dashboard: https://supabase.com/dashboard
# Replace these with your actual project credentials:
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development Settings
NODE_ENV=development
PORT=3000

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=your_ga_id_here
FACEBOOK_PIXEL_ID=your_fb_pixel_id_here

# Contact Information
ADMIN_EMAIL=admin@mentalhealth-ai.bd
SUPPORT_EMAIL=support@mentalhealth-ai.bd

# Security Settings
RATE_LIMIT_MAX_REQUESTS=50
RATE_LIMIT_WINDOW_MS=900000

# Database (if you add backend later)
# DATABASE_URL=mongodb://localhost:27017/mental-health-ai
# DATABASE_NAME=mental_health_ai

# External Services
# SENDGRID_API_KEY=your_sendgrid_key_here
# TWILIO_ACCOUNT_SID=your_twilio_sid_here
# TWILIO_AUTH_TOKEN=your_twilio_token_here

# Feature Flags
ENABLE_CHAT=true
ENABLE_BLOG=true
ENABLE_RESOURCES=true
ENABLE_ANALYTICS=false
